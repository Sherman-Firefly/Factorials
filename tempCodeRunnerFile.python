def factorial(n):
    '''Testing recursion and python documents'''
    if n==0 or n==1:
        return 1
    else:
        return n*factorial(n-1)

print(factorial.__doc__)
print(factorial(5))
print(factorial(6))
print(factorial(7))
print(factorial(8))
print(factorial(9))
print(factorial(10))
print(factorial(50))